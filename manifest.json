{
  "name": "Chrometrast",
  "description": "Make text black again",
  "icons": {
             "16": "icon16.png",
            "128": "icon128.png",
            "256": "icon256.png"
           },
  "version": "1.2",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "commands": {
    "toggle-feature-foo": {
      "suggested_key": {
          "default": "Ctrl+B",
          "mac": "Command+B"
      },
      "description": "Blackify"
    }
  },
  "browser_action": {
    "default_title": "Make text black again"
  },
  "manifest_version": 2
}
